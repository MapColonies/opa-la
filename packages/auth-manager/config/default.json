{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "tracing": {
      "isEnabled": false
    },
    "shared": {}
  },
  "server": {
    "port": 8083,
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "db": {
    "port": 5432,
    "username": "postgres",
    "password": "postgres",
    "ssl": {
      "enabled": false
    },
    "database": "infra-auth-manager-2",
    "schema": "auth_manager"
  }
}
